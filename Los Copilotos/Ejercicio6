function ejercicio3(height) {
    let left = 0;
    let right = height.length - 1;

    let result = 0;

    while ( left < right) {
        let y = Math.min(height[left], height[right]);
        let x = right - left;
        result = (Math.max(result, x*y));
        if (height[right] >= height[left]) {
            left++;
        }
        else {
            right--;
        }
    }
    return result;
}

